<!DOCTYPE html>
<html>
<head>
  <title>中德职教师资培训</title>
  <%= stylesheet_link_tag    'china_germany', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'china_germany', 'data-turbolinks-track' => true %>
  <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
    <%= javascript_include_tag 'html5shiv.min' %>
    <%= javascript_include_tag 'respond.min' %>
  <![endif]-->
  <%= csrf_meta_tags %>
</head>
<body class="<%= controller.controller_path.gsub('/','__') + '__' + controller.action_name %>">
  <%#= render "header"%>

  <!-- flash notice -->
  <%= render :partial => "/common/flash_message" %>
  <!-- flash notice -->
<div class="row">
  <div class="col-xs-12 report-page">
    <div class="row">
    <%= render "china_germany_header" %>
    </div>
      <%= yield %>
    <div class="row">
    <%= render "share/report_footer" %>
    </div>
  </div>
</div>
</body>
</html>
