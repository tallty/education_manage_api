<ol class="breadcrumb">
  <li>
    <%= link_to "后台", admin_panel_root_path  %>
  </li>
  <li class="active" >
    培训班级
  </li>
  <li class="active" >
    负责人反馈
  </li>
</ol>

<div class="grades-main-content">
  <div class="panel panel-primary">
    <div class="panel-heading">
      <h3 class="panel-title">搜索栏</h3>
    </div>
    <div class="panel-body">
      <%= form_tag list_admin_panel_manager_feedbacks_path, method: :get do %>
        <div class="col-sm-3">
          <div class="form-group">
           <%= text_field_tag :keyword, nil, class: "form-control", placeholder: '请输入关键字' %>
          </div>
        </div>
        <div class="col-sm-3">
          <div class="form-group">
            <%= submit_tag "搜索", class: "btn primary" %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
  <div class="panel panel-primary">
    <div class="panel-heading have-menu">
      <ul id="detailMenu" class="nav nav-pills nav-content-menu">
        <%= render "nav"%>
      </ul>
    </div>
    <div class="panel-body">
      <table class="table table-condensed table-bordered table-hover">
	    	<thead>
	    	  <tr>
	    	  	<th class="text-center">负责培训班</th>
	    	  	<th class="text-center">负责人</th>
	    	  	<th class="text-center">承办单位</th>
	    	  	<th class="text-center">总学时</th>
		    		<th class="text-center">总收支</th>
		    		<th class="text-center">总收支明细</th>
		    		<th class="text-center">反馈内容</th>
		    		<th class="text-center">其他情况/备注</th>
		    		<th class="text-center">操作</th>
	    	  </tr>
	    	</thead>
	    	<tbody>
	    	  <%= render @manager_feedbacks %>
		   	</tbody>
	    </table>
    </div>
  </div>
<div>