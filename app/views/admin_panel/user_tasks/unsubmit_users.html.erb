<div class="grades-main-content">
  <div class="panel panel-primary">
    <div class="panel-heading have-menu">
      尚未提交作业人员列表
    </div>

    <div class="panel-body">
      <table class="table table-condensed table-bordered table-hover">
        <thead>
          <tr>
            <th>学员</th>
            <th>联系方式</th>
          </tr>
        </thead>
        <tbody>
          <% @user_training_courses.each do |user_training_course| %>
            <tr>
              <td><%= user_training_course.try(:user).name %></td>
              <td><%= user_training_course.try(:user).try(:email) %><br><%= user_training_course.try(:user).try(:mobile) %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
      <div class="text-center">
        <%= paginate @user_training_courses %>
      </div>
    </div>
  </div>
