<div class="main-content">
	<div class="panel panel-default">
	  <!-- <div class="panel-heading panel-active">评审项目</div> -->
	  <!-- 培训机构的权限是新建申报项目 -->
	  <div class="panel-heading panel-active" style="overflow: hidden;">
	    <span class="pull-left">培训项目</span>
	    <%= render "new_course_resource"%>
	  </div>
	  <div class="panel-body panel-default-bg">
	    <table class="table table-condensed">
	    	<thead>
	    	  <tr>
	    	  	<th class="text-center">所属课程</th>
	    	  	<th class="text-center">标题</th>
	    	  	<th class="text-center">附件</th>
		    		<% if can? :manage, CourseResource %>
		    		  <th class="text-center" colspan="2">操作</th>
		    		<% end %>
	    	  </tr>
	    	</thead>
	    	<tbody>
	    	  <%= render @course_resources %>
		   	</tbody>
	    </table>
	    <div class="text-center">
	    	<%= paginate @course_resources %>
	    </div>
	  </div>
	</div>
</div>